extends ./../../layouts/manage

block content 
 .row
  .large-12.columns
   h2 Add Category
   form(method='post', action='/manage/categories')
    .row
     .large-12.columns
      label Name
      input(type='text', name='name')
    .row
     .large-12.columns
      input.button(type='submit', value='Submit')
      input(type='hidden', name='_csrf', value='{_csrf}')
